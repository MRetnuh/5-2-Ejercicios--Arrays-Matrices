#include <stdio.h>

int main() {
   int v1,v2,v3,v4,v5,v6;
   puts("Ingrese las coordenadas del vector 1: ");
   scanf("%d %d %d", &v1, &v2, &v3);
   puts("Ingrese las coordenadas del vector 2: ");
    scanf("%d %d %d",&v4, &v5, &v6);
    double producto = (v1 * v4) + (v2 * v5) + (v3 * v6);
    printf("El producto entre los vectores es %.2f\n", producto);
    return 0;
}
