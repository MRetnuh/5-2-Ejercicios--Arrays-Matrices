#include <stdio.h>

int main() {
   int n;
   int a[5];
   puts("Ingrese 5 numeros");
   for(int i = 0; i < 5; i++){
       scanf("%d", &n);
       a[i] = n;
     }
     for(int i = 0; i < 5/2; i++){
         int j = 5 - 1 - i;
         int t = a[i];
    a[i] = a[j];
a[j] = t;
     }
     for(int i = 0; i < 5; i++){
         printf("%d ", a[i]);
     }
     printf("\n");
    return 0;
}
