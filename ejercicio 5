// Online C compiler to run C program online
#include <stdio.h>

int main() {
   int dia;
   int mes;
   int a[12] = {31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};
   puts("Ingrese el numero del mes");
   scanf("%d", &mes);
   if(mes < 0 || mes > 12){
       puts("Ingrese un numero valido");
       return 0;
   }
   puts("Ingrese el numero del dia");
   scanf("%d", &dia);
    if(dia < 0 || dia > 31){
       puts("Ingrese un numero valido");
       return 0;
   }
   for(int i = 1; i < mes; i++){
       dia += a[i - 1];
   }
   printf("Faltan %d dias para fin de aÃ±o\n",365 - dia);
    return 0;
}
